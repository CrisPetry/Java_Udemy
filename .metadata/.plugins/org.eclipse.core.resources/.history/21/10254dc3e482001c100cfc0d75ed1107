package fundamentos;

public class Temperatura {

	public static void main(String[] args) {
		
		double F = 109;
		final int ajuste = 32;
		final double fator = 0.5555555555555556;
		double C;
		
		C = (F - ajuste) * fator;
		
		System.out.println("A temperatura em Cº é: " + C);
		
	}
}
